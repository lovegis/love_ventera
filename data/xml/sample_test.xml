			<mrl:source>
				<mrl:LI_Source>
					<mrl:description>
						<gco:CharacterString>The Soil Survey Geographic Database (SSURGO) contains information about soil as collected by the National Cooperative Soil Survey over the course of a century. The information was gathered by walking over the land and observing the soil. Many soil samples were analyzed in laboratories.</gco:CharacterString>
					</mrl:description>
					<mrl:sourceCitation>
						<cit:CI_Citation>
							<cit:title>
								<gco:CharacterString>Soil Survey Geographic Database</gco:CharacterString>
							</cit:title>
							<cit:alternateTitle>
								<gco:CharacterString>SSURGO</gco:CharacterString>
							</cit:alternateTitle>
							<cit:date>
								<cit:CI_Date>
									<cit:date>
										<gco:Date>2023-06-30</gco:Date>
									</cit:date>
									<cit:dateType>
										<cit:CI_DateTypeCode codeList="http://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#CI_DateTypeCode" codeListValue="adopted" />
									</cit:dateType>
								</cit:CI_Date>
							</cit:date>
						</cit:CI_Citation>
					</mrl:sourceCitation>
					<mrl:sourceMetadata>
						<cit:CI_Citation>
							<cit:title>
								<gco:CharacterString>Soil Survey Geographic Database</gco:CharacterString>
							</cit:title>
							<cit:alternateTitle>
								<gco:CharacterString>SSURGO</gco:CharacterString>
							</cit:alternateTitle>
							<cit:date>
								<cit:CI_Date>
									<cit:date>
										<gco:Date>2023-06-15</gco:Date>
									</cit:date>
									<cit:dateType>
										<cit:CI_DateTypeCode codeList="http://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#CI_DateTypeCode" codeListValue="adopted" />
									</cit:dateType>
								</cit:CI_Date>
							</cit:date>
						</cit:CI_Citation>
